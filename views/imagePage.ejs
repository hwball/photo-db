<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Image Uploading</title>
</head>

<body>
  <h1>Uploaded Images</h1>
  <div>
    <% items.forEach(function(image) { %>
      <div>
        <div>
          <img src="data:image/<%=image.file.contentType%>;base64,<%=image.file.data.toString('base64')%>" />
          <div>
            <h5>
              <%= image.filename %>
            </h5>
          </div>
        </div>
      </div>
      <% }) %>
  </div>
</body>

</html>